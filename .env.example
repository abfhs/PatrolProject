# Proxy Configuration (Optional - 프록시 서버 사용시에만 설정)
# PROXY_URL=http://proxy.example.com:8080

# Site Credentials (인터넷등기소 접속 정보 - 필수)
SITE_ID=your_site_id_here
SITE_PASSWORD=your_site_password_here

# JWT Configuration (JWT 토큰 암호화 키 - 필수)
JWT_SECRET=your-32-character-long-secret-key-here

# Database Configuration (PostgreSQL 설정 - 선택사항, 기본값 사용 가능)
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_USER=postgres
DATABASE_PASSWORD=your_db_password
DATABASE_NAME=patrol

# Admin User Configuration (관리자 계정 설정 - 필수)
ADMIN_EMAIL=admin@example.com
ADMIN_PASSWORD=your_secure_admin_password
ADMIN_NICKNAME=Admin

# Email Configuration (Gmail SMTP 설정 - 이메일 인증 기능 사용시 필수)
MAIL_HOST=smtp.gmail.com
MAIL_PORT=587
MAIL_USER=your-gmail@gmail.com
MAIL_PASS=your-gmail-app-password
MAIL_FROM=your-gmail@gmail.com

# Base URL Configuration (로컬 개발환경 설정)
BASE_URL=http://localhost:3000
FRONTEND_URL=http://localhost:5173